{
  "name": "ngxsmk-datepicker-workspace",
  "version": "2.1.6",
  "description": "A lightweight, customizable, and easy-to-use datepicker and date range picker for Angular applications.",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve demo-app",
    "build": "npx ng build",
    "build:demo": "npx ng build demo-app --configuration production",
    "build:optimized": "npx ng build ngxsmk-datepicker --configuration production && node scripts/remove-sourcemaps.js",
    "build:analyze": "node scripts/analyze-bundle.js",
    "optimize": "node scripts/optimize-build.js",
    "watch": "npx ng build --watch --configuration development",
    "test": "npx ng test ngxsmk-datepicker",
    "test:coverage": "npx ng test ngxsmk-datepicker --code-coverage --watch=false --browsers=ChromeHeadless",
    "demo": "npx ng serve demo-app",
    "lint": "npx ng lint",
    "deploy": "npm run build:demo && node scripts/manual-deploy.js",
    "prepublishOnly": "npm run build:optimized",
    "prepublish:copy-assets": "node -e \"const fs = require('fs'); const files = ['LICENSE', 'README.md', 'CHANGELOG.md', 'MIGRATION.md', 'CONTRIBUTING.md', 'CODE_OF_CONDUCT.md', 'ROADMAP.md', 'SECURITY.md']; files.forEach(f => { if (fs.existsSync(f)) fs.copyFileSync(f, 'dist/ngxsmk-datepicker/' + f); });\"",
    "publish:lib": "npm run build:optimized && npm run prepublish:copy-assets",
    "unpublish:version": "node scripts/unpublish-version.js",
    "version:beta": "node scripts/set-beta-version.js next",
    "version:stable": "node scripts/set-stable-version.js",
    "publish:beta": "npm run build:optimized && npm run prepublish:copy-assets && cd dist/ngxsmk-datepicker && npm publish --tag beta",
    "publish:patch": "npm run build:optimized && npm run prepublish:copy-assets && cd dist/ngxsmk-datepicker && npm publish --tag latest",
    "semantic-release": "semantic-release",
    "docs": "typedoc --options typedoc.json && node scripts/enhance-api-docs.js",
    "docs:serve": "npm run docs && npx http-server docs/api -p 8080 -o",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@angular/common": ">=17.0.0 <24.0.0",
    "@angular/core": ">=17.0.0 <24.0.0",
    "@angular/forms": ">=17.0.0 <24.0.0",
    "luxon": "^3.0.0"
  },
